    "chrome",
    "headless",
    "pupeteer"
  ],
  "devDependencies": {
    "ava": "3.15.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^8.0.0"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "puppeteer-extra-plugin": "^3.1.9"
  },
