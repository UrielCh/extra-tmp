{
  "name": "puppeteer-extra-plugin-devtools",
  "version": "2.2.17",
  "description": "Make puppeteer browser debugging possible from anywhere (devtools with screencasting on the internet).",
  "main": "dist/index.js",
  "repository": "berstend/puppeteer-extra",
  "author": "berstend",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist/*",
    "tscheck": "tsc --pretty --noEmit",
    "prebuild": "run-s clean",
    "build": "run-s build:tsc",
    "build:tsc": "tsc --module commonjs",
    "build:rollup": "rollup -c rollup.config.ts",
    "docs": "node -e 0",
    "lint": "eslint --ext .js .",
    "test:ts": "ava -v --serial --concurrency 1 --fail-fast --config ava.config-ts.js",
    "test:js": "ava -v --serial --concurrency 1 --fail-fast",
    "test": "node -e 0",
    "pretest-ci": "run-s build",
    "test-ci": "run-s test"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "puppeteer",
    "puppeteer-extra",
    "puppeteer-extra-plugin",
    "devtools",
    "devtools-tunnel",
    "localtunnel",
    "remote-debugging",
    "chrome",
    "headless",
    "pupeteer"
  ],
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/http-proxy": "^1.17.5",
    "@types/url-parse": "^1.4.3",
    "ava": "3.15.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "8.0.0",
    "puppeteer-extra": "^3.1.18",
    "types": "^0.1.1"
  },
  "dependencies": {
    "@types/http-auth": "^4.1.0",
    "@types/localtunnel": "^2.0.0",
    "@types/randomstring": "^1.1.6",
    "debug": "^4.1.1",
    "get-port": "^3.2.0",
    "got": "^11.8.2",
    "http-auth": "^4.1.4",
    "http-proxy": "^1.17.0",
    "http-proxy-response-rewrite": "^0.0.1",
    "localtunnel": "^2.0.0",
    "ow": "^0.23.0",
    "puppeteer-extra-plugin": "^3.1.9",
    "randomstring": "^1.1.5",
    "url-parse": "^1.4.3"
  },
  "gitHead": "babb041828cab50c525e0b9aab02d58f73416ef3"
}
